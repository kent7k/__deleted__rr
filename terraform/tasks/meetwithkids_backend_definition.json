[
  {
    "name": "backend-container",
    "image": "700347967260.dkr.ecr.ap-northeast-1.amazonaws.com/meetwithkids-backend",
    "essential": true,
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-region": "ap-northeast-1",
        "awslogs-stream-prefix": "backend",
        "awslogs-group": "/ecs/backend"
      }
    },
    "portMappings": [
      {
        "protocol": "tcp",
        "containerPort": 3000,
        "hostPort": 3000
      }
    ],
    "environment": [
      {
        "name": "API_DOMAIN",
        "value": "https://meetwithkids.org"
      },
      {
        "name": "WORKDIR",
        "value": "app"
     },
      {
        "name": "CONTAINER_PORT",
        "value": "3000"
      },
      {
        "name": "RAILS_ENV",
        "value": "production"
      }
    ],
    "secrets": [
      {
        "name": "DATABASE_URL",
        "valueFrom": "terraform-20230225033756705300000001.cbch251i7aoz.ap-northeast-1.rds.amazonaws.com"
      },
      {
        "name": "RAILS_MASTER_KEY",
        "valueFrom": "567f18a0f3d227d0a02bd94645378c2b"
      }
    ]
  }
]
